{
  "name": "client-qwik",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build.client": "tsc && vite build && npm run i18n-translate",
    "i18n-extract": "node_modules/.bin/localize-extract -s server/entry.express.js -f json -o public/locale/message.en.json",
    "i18n-translate": "node_modules/.bin/localize-translate -s entry.express.js -t public/locale/message.en.json -o server/{{LOCALE}} -r server",
    "serve.en": "node server/en/entry.express"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "i18n-config": "*",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "@angular/localize": "^16.2.0",
    "@builder.io/qwik": "^1.2.6",
    "vite-plugin-svgr": "^4.1.0"
  }
}
